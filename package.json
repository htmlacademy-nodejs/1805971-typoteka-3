{
  "name": "typoteka",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production LOG_LEVEL=error node ./src/service/service.js --server 3001",
    "start::debug": "cross-env NODE_ENV=development LOG_LEVEL=error nodemon ./src/service/service.js --server 3001",
    "generate": "node ./src/service/service.js --generate 5",
    "server": "cross-env NODE_ENV=development LOG_LEVEL=error nodemon ./src/service/service.js --server",
    "start-frontend-server": "nodemon ./src/express/index.js",
    "test": "npm run eslint && npm run jest",
    "jest": "jest --watchAll --coverage ",
    "test::eslint": "npm run eslint",
    "eslint": "eslint --ext .js src/",
    "fix": "eslint src/ --ext .js --fix"
  },
  "repository": {
    "start": "node ./src/service/service.js",
    "type": "git",
    "url": "git+https://github.com/AntonVolkov71/1805971-typoteka-3.git"
  },
  "author": "Anton Volkov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AntonVolkov71/1805971-typoteka-3/issues"
  },
  "homepage": "https://github.com/AntonVolkov71/1805971-typoteka-3#readme",
  "devDependencies": {
    "eslint": "7.26.0",
    "eslint-config-htmlacademy": "0.6.0",
    "jest": "27.0.6",
    "nodemon": "2.0.7",
    "pino-pretty": "5.1.2",
    "supertest": "6.1.4"
  },
  "dependencies": {
    "axios": "0.21.4",
    "chalk": "4.1.1",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "multer": "^1.4.3",
    "nanoid": "3.1.23",
    "pino": "6.13.0",
    "pug": "2.0.4"
  }
}
